      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Destination recommendation returned', function () {",
              "    pm.response.to.have.status(200);",
              "    const response = pm.response.json();",
              "    pm.expect(response).to.have.property('destination');",
              "    pm.expect(response).to.have.property('country');",
              "    pm.expect(response).to.have.property('description');",
              "    pm.expect(response).to.have.property('bestTimeToVisit');",
              "});",
              "",
              "pm.test('Destination has complete information', function () {",
              "    const response = pm.response.json();",
              "    pm.expect(response.destination).to.be.a('string');",
              "    pm.expect(response.description.length).to.be.at.least(100);",
              "    pm.expect(response).to.have.property('attractions');",
              "});"
