    {
      "name": "‚ùå Error Scenario Testing",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Error testing completed', function () {",
              "    pm.response.to.have.status(200);",
              "    const response = pm.response.json();",
